<Window x:Class="Hearthstone_Deck_Tracker.HearthStats.HearthStatsTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="HearthStatsTestWindow" Height="300" Width="300">
    <StackPanel>
        <Button Content="Send deck" VerticalAlignment="Top" Click="ButtonBase_OnClick"/>
        <Button Name="BtnGetDecks" Content="get decks" Click="BtnGetDecks_OnClick"/>
        <Button x:Name="BtnPostGame" Content="post game" Click="BtnPostGame_OnClick"/>
        <Button x:Name="BtnPosFullDeck" Content="full deck" Click="BtnPosFullDeck_OnClick"/>
        <Button x:Name="BtnPostAll" Content="POST ALL THE THINGS" Click="BtnPostAll_OnClick"/>
    </StackPanel>
</Window>
